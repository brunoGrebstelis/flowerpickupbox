<!-- PRICING & ROI SECTION -->
<section id="pricing" aria-labelledby="pricing-heading">
  <div class="wrapper">
    <h2 id="pricing-heading">{{ t.pricingHeading }}</h2>
    <div class="pricing-card">
      <p id="i18n-pricingSubheading" class="pricing-sub">{{ t.pricingSubheading }}</p>
      <p id="i18n-pricingBody1">{{ t.pricingBody1 }}</p>
      <p id="i18n-pricingBody2">{{ t.pricingBody2 }}</p>
      <p id="i18n-pricingBody3">{{ t.pricingBody3 }}</p>
      <div class="pricing-cta">
        <a class="msg-btn msg-btn--b" id="i18n-pricingCta" href="#contacts">{{ t.pricingCta }}</a>
      </div>
    </div>
  </div>
</section>
